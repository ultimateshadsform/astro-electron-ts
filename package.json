{
  "name": "astro-electron-ts-monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "bun run --cwd packages/* build",
    "dev": "bun run --cwd packages/* dev",
    "test": "bun run --cwd packages/* test",
    "clean": "bun rimraf packages/*/dist"
  },
  "devDependencies": {
    "rimraf": "6.0.1"
  }
}
