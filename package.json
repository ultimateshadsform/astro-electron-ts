{
  "name": "astro-electron-ts-monorepo",
  "description": "Monorepo for Astro Electron TS",
  "repository": "github:ultimateshadsform/astro-electron-ts",
  "homepage": "https://github.com/ultimateshadsform/astro-electron-ts",
  "license": "MIT",
  "author": "SHADXW",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "bun run build:create && bun run build:integration",
    "build:create": "bun run --cwd packages/create-astro-electron-ts build",
    "build:integration": "bun run --cwd packages/astro-electron-ts build",
    "dev": "bun run dev:create && bun run dev:integration",
    "dev:create": "bun run --cwd packages/create-astro-electron-ts dev",
    "dev:integration": "bun run --cwd packages/astro-electron-ts dev",
    "test": "bun run test:create && bun run test:integration",
    "test:create": "bun run --cwd packages/create-astro-electron-ts test",
    "test:integration": "bun run --cwd packages/astro-electron-ts test",
    "clean": "bun rimraf packages/*/dist"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "eslint": "^9.14.0",
    "rimraf": "6.0.1"
  }
}
