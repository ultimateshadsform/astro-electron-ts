{
  "name": "astro-electron-ts",
  "description": "Build cross-platform desktop apps with Electron and Astro",
  "repository": "github:ultimateshadsform/astro-electron-ts",
  "homepage": "https://github.com/ultimateshadsform/astro-electron-ts",
  "license": "MIT",
  "author": "SHADXW",
  "type": "module",
  "version": "1.0.21",
  "keywords": [
    "withastro",
    "astro-integration",
    "astro",
    "electron"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "vitest": "2.1.5",
    "typescript": "^5.3.3",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "vite-plugin-electron": "^0.28.8"
  },
  "peerDependencies": {
    "astro": "^4.0.0",
    "electron": ">=22.0.0"
  }
}
